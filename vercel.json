{
  "rewrites": [
    {
      "source": "/sparta/:path*",
      "destination": "https://sparta.hmif.dev/sparta/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Document-Policy",
          "value": "js-profiling"
        }
      ]
    }
  ]
}
